  {% extends "default.njk" %}
  
  {% block content %}
  <article  class="h-entry">
      <header>

        <a href="{{ path }}" class="u-url"><h1 class="e-content p-name">{% if realtitle %}{{ realtitle }}{% else %}{{ title }}{% endif %}</h1></a>
        <p>{{ strapline }}</p>
        <time class="dt-published">{{ date }}</time>
      </header>
      {{ contents | safe }}
  </article>
  {% endblock %}