{% extends "default.njk" %}
{% block list %}

    <ul class="comp-contentlist">
    {% for note in notes %}
        <li class="comp-contentlist__item">
            <a href="{{ note.path }}" class="comp-contentlist__link">
                <p class="comp-contentlist__date">{{ note.date }}</p>
            </a>
        </li>
    {% endfor %}
    </ul>
{% endblock %}